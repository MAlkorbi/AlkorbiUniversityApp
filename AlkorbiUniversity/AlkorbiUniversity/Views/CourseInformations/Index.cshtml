@model IEnumerable<AlkorbiUniversity.Models.CourseInformation>

@{
    ViewBag.Title = "Course Information";
}

<h2>Course Information</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Credits)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Level)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Aim)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PreRequisites)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartingDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FinishingDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Credits)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Level)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Aim)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PreRequisites)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartingDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FinishingDate)
        </td>
        <td>
           
            @Html.ActionLink("Details", "Details", new { id=item.CourseInformationID }) 
            
        </td>
    </tr>
}

</table>
