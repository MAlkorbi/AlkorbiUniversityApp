@model IEnumerable<AlkorbiUniversity.Models.UserAccount>

@{
    ViewBag.Title = "Registered Users Information";
}

<h2>Registered Users Information</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FristName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
               
        
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FristName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
       
        
    </tr>
}

</table>
